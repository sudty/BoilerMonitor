<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>

	<appSettings>
		<!--真实硬件-->
		<add key="dewPoint.serial.PortName" value="COM7"/>
		<add key="dewPoint.serial.BauRate" value="9600"/>
		<add key="dewPoint.serial.DataBits" value="8"/>
		<add key="dewPoint.serial.StopBits" value="One"/>
		<add key="dewPoint.serial.Parity" value="None"/>
		
		<!--温湿度-->
		<add key="dewPoint.temperature.slave" value="2"/>
		<!--数码管-->
		<add key="dewPoint.digitalTube.slave" value="1 "/>

		<!--模拟配置-->
		<add key="boiler.serial.PortName" value="COM1"/>
		<add key="boiler.serial.BauRate" value="9600"/>
		<add key="boiler.serial.DataBits" value="8"/>
		<add key="boiler.serial.StopBits" value="One"/>
		<add key="boiler.serial.Parity" value="None"/>
		
		<!--锅炉-->
		<add key="boiler.slave" value="1"/>
		<!--水温上限-->
		<add key="boiler.temperature.upper" value="100"/>
		<!--水温下限-->
		<add key="boiler.temperature.lower" value="90"/>
	</appSettings>
</configuration>